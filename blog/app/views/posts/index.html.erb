<h1>Hello Rails</h1>
<%= link_to "My Blog", controller: "posts" %>

<%= link_to 'New post', new_post_path %> 
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
  </tr>
 
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.text %></td>
    </tr>
  <% end %>
</table>